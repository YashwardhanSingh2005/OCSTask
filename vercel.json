{
    "version":   2,
    "builds": [
      {
        "src": "./Backend.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/Backend.js"
      },
      {
        "src": "/views/(.*)",
        "dest": "/views/$1"
      }
    ],
    "rewrites": [
      {
        "source": "/(.*)",
        "destination": "/"
      }
    ]
}

